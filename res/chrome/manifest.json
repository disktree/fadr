{
    "name": "Fadr",
    "version": "1.0.20",
    "description": "Color fading environment beautifier",
    "manifest_version": 2,
    "minimum_chrome_version": "45",
    "offline_enabled": true,
    "short_name": "Fadr",
    "app": {
        "background": { "scripts": ["background.js"] }
    },
    "permissions": ["idle","storage","power","system.display","app.window.alwaysOnTop"],
    "icons": {
        "16": "icon/fadr-desktop-16.png",
        "48": "icon/fadr-desktop-48.png",
        "128": "icon/fadr-desktop-128.png"
    }
}
