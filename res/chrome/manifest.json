{
    "name": "Fadr",
    "version": "1.0.7",
    "description": "Color fading screensaver",
    "manifest_version": 2,
    "minimum_chrome_version": "44",
    "offline_enabled": true,
    "short_name": "Fadr",
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "permissions": ["idle","storage","power","system.display","app.window.alwaysOnTop"],
    "icons": {
        "16": "icon/chrome-16.png",
        "48": "icon/chrome-48.png",
        "128": "icon/chrome-128.png"
    }
}
